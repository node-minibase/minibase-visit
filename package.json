{
  "name": "minibase-visit",
  "version": "1.0.5",
  "description": "Plugin for [minibase][] and [base][], that adds `.visit` method to your application to visit a method over the items in an object, or map visit over the objects in an array. Using using [collection-visit][] package.",
  "repository": "node-minibase/minibase-visit",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://i.am.charlike.online)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100 --functions 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "collection-visit": "^0.2.3",
    "minibase-create-plugin": "^1.0.3"
  },
  "devDependencies": {
    "commitizen": "~2.9.2",
    "coveralls": "^2.11.15",
    "cz-conventional-changelog": "1.1.5",
    "minibase": "^1.0.0",
    "mukla": "^0.4.8",
    "nyc": "^10.0.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.6.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "array",
    "arrays",
    "base",
    "baseplugin",
    "call",
    "called",
    "context",
    "core",
    "ctx",
    "function",
    "helper",
    "invoke",
    "key",
    "map",
    "mapper",
    "method",
    "minibase",
    "minibaseplugin",
    "object",
    "objects",
    "recursive",
    "value",
    "visit",
    "visitor"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "minibase",
        "minibase-is-registered",
        "base-task-alias",
        "try-catch-core",
        "minibase-results",
        "minibase-assert",
        "minibase-tests",
        "minibase-create-plugin",
        "minibase-better-define"
      ]
    },
    "reflinks": [
      "always-done",
      "base",
      "collection-visit",
      "dezalgo",
      "minibase",
      "once",
      "assert-kindof",
      "commitizen",
      "standard-version",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
